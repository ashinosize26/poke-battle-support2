<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GHPages>true</GHPages>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="元データ\**" />
    <Content Remove="元データ\**" />
